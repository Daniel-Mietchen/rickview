@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ex: <http://example.com/resource/> .

ex:ExampleClass
    a owl:Class ;
    rdfs:comment "This is an example class."@en ;
    rdfs:label "example class"@en .

ex:ExampleInstance
    ex:exampleProperty 5 ;
    a ex:ExampleClass ;
    rdfs:comment "This is an example instance."@en ;
    rdfs:label "example instance"@en .

ex:exampleProperty
    a owl:DatatypeProperty ;
    rdfs:domain ex:ExampleClass ;
    rdfs:label "Beispielproperty"@de, "example property"@en .

